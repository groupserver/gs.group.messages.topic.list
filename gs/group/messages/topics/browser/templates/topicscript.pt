<script 
  id="gs-group-messages-topics-script"
  type="text/javascript">
  jQuery.noConflict();
  jQuery(document).ready( function () {
    var topicsSearch = null;
    var b = null;
    var url = null;

    b = jQuery('base').attr('href');
    if (b[b.length -1] != '/') {
      b = b + '/';
    }
    url = b + 'gs-group-topics-ajax.html';
    topicsSearch = GSSearch('#gs-group-messages-topics-search', 
                            url, 0, 6, {}, null);
    topicsSearch.load();
    jQuery('#gs-group-messages-topics-search').on('resultsloaded', function () {
    jQuery('abbr[rel=tooltip]').tooltip();
    });
  });
</script><!--gs-group-messages-topics-script-->
